{
    "name": "@geedmo/yamm",
    "version": "4.0.1",
    "description": "Yet another megamenu for Bootstrap",
    "homepage": "http://geedmo.github.io/yamm/",
    "author": "@geedmo",
    "main": "dist/yamm.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/geedmo/yamm"
    },
    "scripts": {
        "build-sass-compressed": "node-sass src/yamm.scss dist/yamm.min.css --output-style compressed",
        "build-sass-expanded": "node-sass src/yamm.scss dist/yamm.css --output-style expanded",
        "build-sass": "npm run build-sass-expanded && npm run build-sass-compressed",
        "build-pug": "pug demo/pug/index.pug -o . -P",
        "build": "npm run build-sass && npm run build-pug",
        "test": "tap test/basic.js",
        "serve": "live-server --watch=\"dist,index.html\"",
        "watch:build-sass-compressed": "node-sass src/yamm.scss dist/yamm.min.css --output-style compressed --watch",
        "watch:build-sass-expanded": "node-sass src/yamm.scss dist/yamm.css --output-style expanded --watch",
        "watch:build-pug": "pug demo/pug/index.pug -o . --watch",
        "watch": "npm run build && concurrently \"npm run serve\" \"npm run watch:build-sass-compressed\" \"npm run watch:build-sass-expanded\" \"npm run watch:build-pug\""
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "less": "^4.2.0",
        "live-server": "^1.2.2",
        "node-sass": "^9.0.0",
        "pug": "^3.0.2",
        "pug-cli": "^1.0.0-alpha6",
        "stylus": "^0.62.0",
        "tap": "^18.7.0"
    },
    "nyc": {
        "temp-dir": "./node_modules/.nyc_output"
    }
}
